[core]
    editor = vim 
[alias]
    cl = clone
    co = checkout
    ci = commit
    st = status
    br = branch
    df = diff
    pl = pull
    ps = push
    lg = log --color --graph --pretty=format:'%Cred%h%Creset \
            -%C(yellow)%d%Creset %s %Cgreen (%cr) %C(bold blue)<%an>%Creset' \
            --abbrev-commit --

